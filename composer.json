{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "laravel/framework": "~5.0",
        "pda/pheanstalk": "~3",
        "tokenly/counterparty-transaction-parser": "dev-master@dev",
        "tokenly/counterparty-asset-info-cache": "dev-master@dev",
        "tokenly/xchain-listener": "dev-master@dev",
        "tokenly/bitcoin-address-lib": "dev-master@dev",
        "tokenly/bitcoin-currency-lib": "dev-master@dev",
        "tokenly/xcpd-client": "dev-master@dev",
        "tokenly/hmac-auth": "dev-master@dev",
        "tokenly/insight-client": "dev-master@dev",
        "tokenly/token-generator": "dev-master@dev",
        "nc/faye-client": "dev-master",
        "rhumsaa/uuid": "~2.",
        "linusu/bitcoin-address-validator": "~0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "satooshi/php-coveralls": "dev-master",
        "psr/log": "~1"
    },
    "autoload": {
        "classmap": [
            "database",
            "tests/lib/",
            "tests/TestCase.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan route:scan",
            "php artisan event:scan",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "auto"
    },
    "minimum-stability": "dev"
}
